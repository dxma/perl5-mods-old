bool 
[% package %]::remove(...)
PROTOTYPE: ;$
PREINIT:
    [% type %] *p;
    unsigned int i;
CODE:
    /*!
     * bool remove(uint index)
     * bool remove()
     * bool remove(const type * item)
    */
    switch(items) {
    case 2:
        if(sv_isobject(ST(1)) && sv_isa(ST(1), "[% type_package %]")) {
            p = reinterpret_cast<const [% type %] *>(SvIV(SvRV(ST(1))));
            RETVAL = THIS->remove(p);
            // no delete if auto delete enabled
            if(RETVAL == true && THIS->autoDelete())
                SvREADONLY_on(SvRV(ST(1)));
        } else if(SvIOK(ST(1))) {
            i = SvIV(ST(1));
            RETVAL = THIS->remove(i);
        } else 
            croak("ST(1) is not of type [% type_package %] or integer");
        break;
    default:
        RETVAL = THIS->remove();
    }
OUTPUT:
    RETVAL

