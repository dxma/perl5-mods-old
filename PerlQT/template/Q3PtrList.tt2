static void 
[% package %]::TIEARRAY(...)
PROTOTYPE: ;$
PREINIT:
	Q3PtrList<[% type %]> * list;
PPCODE:
	/*!
	 * tie @a, "[% package %]"
	 * tie @a, "[% package %]", $obj_to_tie
	 */
	switch(items) {
	case 2:
		if(sv_isobject(ST(1)) && sv_derived_from(ST(1), "[% package %]")) {
			ST(0) = sv_2mortal(newRV_inc(SvRV(ST(1))));
		} else
			croak("ST(1) is not of type [% package %]");
		break;
	default:
		/* items == 1 */
		list = new Q3PtrList<[% type %]>();
		ST(0) = sv_newmortal();
		sv_setref_pv(ST(0), "[% package %]", reinterpret_cast<void *>(list));
	}
	XSRETURN(1);

void 
[% package %]::FETCH(index)
	unsigned int index
PREINIT:
    [% type %] *item;
PPCODE:
	if(0 <= index && index < THIS->count()) {
		ST(0) = sv_newmortal();
		item = THIS->at(index);
		sv_setref_pv(ST(0), "[% type_package %]", reinterpret_cast<void *>(item));
        // export only
		SvREADONLY_on(SvRV(ST(0)));
		XSRETURN(1);
	} else
		XSRETURN_UNDEF;

void 
[% package %]::STORE(index, item)
	unsigned int index
	const [% type %] *item
CODE:
	/* 
	 * insert item into specific index 
	 * append to the end if index out of bound 
	 */
    if( 0 <= index && index < THIS->count()) {
        // mark readonly
        if(THIS->insert(index, item)) 
            SvREADONLY_on(SvRV(ST(2)));
    } else {
        THIS->append(it);
        // mark readonly
        SvREADONLY_on(SvRV(ST(2)));
    }

