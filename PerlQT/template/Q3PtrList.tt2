MODULE = [% module %]			PACKAGE = [% package %]
PROTOTYPES: ENABLE

################################################################
#### 
#### NB: $type is *always* object
#### 
#### $type           : item type
#### $package        : package namespace
#### $type_package   : item namespace
#### $root_namespace : universal namespace
####
################################################################

Q3PtrList<[% type %]> * 
[% package %]::new(...)
PROTOTYPE: ;$
PREINIT:
    [% package %] *p;
CODE:
    /* 
     * Q3PtrList<[% type %]>()
     * Q3PtrList<[% type %]>(const Q3PtrList<[% type %]> &list)
     */
    switch(items) {
    case 2:
        if(sv_isobject(ST(1)) && sv_derived_from(ST(1), "[% package %]"))
            p = reinterpret_cast<Q3PtrList<[% type %]> *>(SvIV(SvRV(ST(1))));
        else
            croak("ST(1) is not of type [% package %]");
        RETVAL = new Q3PtrList<[% type %]>(*p);
        break;
    default:
        RETVAL = new Q3PtrList<[% type %]>();
        // set autoDelete true by default 
        RETVAL->setAutoDelete(true);
        // _PERLQT_TODO: SvREADONLY_off(RETVAL)
    }
OUTPUT:
    RETVAL

void 
[% package %]::DESTROY()
CODE:
    // delete that not marked with readonly
    if(!SvREADONLY(SvRV(ST(0))))
        delete THIS;

void
[% package %]::append(item)
    const [% type %] *item
CODE:
    THIS->append(item);
    // mark item readonly
    // since its underlying object has been taken over by current list
    SvREADONLY_on(SvRV(ST(1)));

void 
[% package %]::at(index)
    uint index
PREINIT:
    [% type %] *item;
PPCODE:
    /*! 
     * [% type %] *Q3PtrList::at(uint index) 
     * int Q3PtrList::at()
    */
    switch(items) {
    case 2:
	    item = THIS->at(index);
        if (item != 0) {
	        ST(0) = sv_newmortal();
	        sv_setref_pv(ST(0), "[% type_package %]", reinterpret_cast<void *>(item));
            // export only, still owned by list
	        SvREADONLY_on(SvRV(ST(0)));
        } 
        else {
            ST(0) = &PL_sv_undef;
        }
        break;
    default:
        ST(0) = sv_2mortal(newSViv(THIS->at()));
    }
    XSRETURN(1);

